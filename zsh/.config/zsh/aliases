#!/bin/zsh

alias US="vim /mnt/ext/Notes/US_Plan/index.wiki"
alias cpusage="ps -axch -o cmd,%mem --sort=-%mem | head"
alias ll="ls -l"
alias la="ls -la"
alias ls="ls --color=always"
alias sxf="startx ~/.xinitrc xfce"
alias sx="startx ~/.xinitrc dwm"
alias searx="cd ~/.local/share/nativefier/searx-linux-x64 && ./searx"
alias whatsapp="cd ~/.local/share/nativefier/WhatsAppWeb-linux-x64 && ./WhatsAppWeb"
alias todo="vim /mnt/ext/Media/dox/todo/school.wiki"

alias dw="cd /mnt/ext/dwn/"
alias dd="cd /mnt/ext/Media/dox/"
alias dp="cd /mnt/ext/Media/Pix/"
alias dv="cd /mnt/ext/Media/vids/"
alias dm="cd /mnt/ext/Media/vids/"
alias db="cd /mnt/ext/Media/Book/"
alias dg="cd /mnt/ext/Huge\ Files/Games/"
alias c="lf ~/ghub/dotfiles/"
alias wbc="ffplay -left 10 -f v4l2 -fflags nobuffer -flags low_delay -video_size 640x480 -framerate 60 -i /dev/video0 2&>/dev/null"
